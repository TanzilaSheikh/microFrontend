{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "microfrontend": {
      "projectType": "library",
      "root": "projects/microfrontend",
      "sourceRoot": "projects/microfrontend/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/microfrontend/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/microfrontend/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/microfrontend/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/microfrontend/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "microfrontendLibrary": {
      "projectType": "library",
      "root": "projects/microfrontend-library",
      "sourceRoot": "projects/microfrontend-library/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/microfrontend-library/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/microfrontend-library/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/microfrontend-library/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/microfrontend-library/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "7e031a47-f1ff-47e1-8aad-ca36acb382d8"
  }
}
